{
  "name": "leetcode-ts",
  "version": "1.0.0",
  "description": "Collection of LeetCode solutions implemented in TypeScript for algorithm practice.",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "dev": "vitest --watch",
    "lint": "eslint --ext .ts src",
    "fix": "eslint --ext .ts src --fix && prettier --write .",
    "new": "tsx scripts/new-problem.ts",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "generate:readme": "tsx scripts/update-readme.ts"
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run"
    ]
  },
  "keywords": [
    "leetcode",
    "algorithms",
    "typescript",
    "data-structures",
    "interview-prep",
    "solutions"
  ],
  "author": "Olimjonov Otabek olimjonovpublic@gmail.com (https://github.com/OlimjonovOtabek)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OlimjonovOtabek/leetcode-ts.git"
  },
  "homepage": "https://github.com/OlimjonovOtabek/leetcode-ts#readme",
  "bugs": {
    "url": "https://github.com/OlimjonovOtabek/leetcode-ts/issues"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
